<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.stage.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>

<GridPane fx:id="rootPane" xmlns:fx="https://javafx.com/fxml" fx:controller="ctc.RemoveTrainFormController" hgap="10" vgap="10">
	<padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
	<Text text="Train: " GridPane.rowIndex="0" GridPane.columnIndex="0" />
	<ComboBox fx:id="trainListBox"  GridPane.rowIndex="0" GridPane.columnIndex="1" />
	<HBox GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.columnSpan="2" spacing="10">
		<Button text="Submit" onAction="#submit" />
		<Button text="Cancel" onAction="#cancel" />
	</HBox>
</GridPane>